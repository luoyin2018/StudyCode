<PageFunction
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib" 
    x:Class="NavigationPage.SelectPageFunction"
    x:TypeArguments="sys:String"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:NavigationPage"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="300"
    Title="SelectPageFunction">
    <StackPanel>
        <TextBlock>选择一项:</TextBlock>
        <ListBox x:Name="listbox">
            <ListBoxItem>C# 3.0</ListBoxItem>
            <ListBoxItem>WPF</ListBoxItem>
            <ListBoxItem>Java</ListBoxItem>
            <ListBoxItem>HaHa</ListBoxItem>
        </ListBox>
        <StackPanel Orientation="Horizontal">
            <Button Click="btnOk_click">确定</Button>
            <Button Click="btnCancel_click">取消</Button>
        </StackPanel>
    </StackPanel>
</PageFunction>
